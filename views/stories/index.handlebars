<h1>Stories</h1>

<div class="row">
    {{#each stories}}
    <div class="col s12 m4">
        <div class="card">
            <div class="card-content center-align">
                <h5>{{title}}</h5>
            </div>
        </div>
    </div>
    {{else}}
    <p>No Stories found</p>
    {{/each}}
</div>